/* Dark Theme */
QMainWindow {
    background-color: #333333;
    color: #f0f0f0;
}
QLabel {
    color: #f0f0f0;
}
QPushButton {
    background-color: #555555;
    color: #f0f0f0;
    border: 1px solid #777777;
    padding: 5px;
    border-radius: 3px;
}
QPushButton:hover {
    background-color: #666666;
}
QTextEdit {
    background-color: #222222;
    color: #f0f0f0;
    border: 1px solid #555555;
}
